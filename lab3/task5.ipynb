   "source": [
    "import numpy as np\n",
    "import timeit\n",
    "\n",
    "d1 = np.random.rand(100, 1)\n",
    "A = np.random.rand(100, 200)\n",
    "\n",
    "D1 = np.diag(d1.ravel())\n",
    "\n",
    "def a():\n",
    "    return D1 @ A   \n",
    "def b():\n",
    "    return d1 * A\n",
    "\n",
    "print(timeit.timeit(a, number=1000))\n",
    "print(timeit.timeit(b, number=1000))"
   ]
  }
 ],
